<%= semantic_form_for @nitak_table do |f| %>
  <%= f.inputs do %>
    <%= f.input :activity_table %>
    <%= f.input :company_status %>
    <%= f.input :fixed_value %>
    <%= f.input :yearly_value %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
